	<subDataset name="dataset1" uuid="21910617-dca3-4f0a-834c-775bf7e79de9">
		<queryString>
			<![CDATA[SELECT detalle_venta.cantidad AS 'Cantidad', producto.nombre AS 'Detalle', 
TRUNCATE (detalle_venta.`precio`,2 )AS 'P.Unitario', TRUNCATE ((`detalle_venta`.`cantidad` * `detalle_venta`.`precio`),2)AS 'P.total' 
FROM detalle_venta INNER JOIN producto
ON
detalle_venta.id_producto = producto.id
]]>
		</queryString>
		<field name="Cantidad" class="java.lang.Long">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="Detalle" class="java.lang.String"/>
		<field name="P.Unitario" class="java.math.BigDecimal"/>
		<field name="P.total" class="java.math.BigDecimal"/>
	</subDataset>
	<queryString>
		<![CDATA[SELECT `cabecera_venta`.`num_venta`,empresa.`nombre` AS Empresa, empresa.`ruc`,empresa.`direccion`, 
CONCAT (usuario.`nombres`," ",usuario.`apellidos`)AS 'Nombre_completos',
usuario.`cedula`, 
DATE_FORMAT (cabecera_venta.`fecha_creacion`,'%d %M %Y') AS 'Fecha',
`cabecera_venta`.`tipo_pago`,
TRUNCATE (`cabecera_venta`.`subtotal_con_iva`,2) AS 'Subtotal_con_iva',
TRUNCATE (cabecera_venta.`subtotal_sin_iva`,2)AS 'Subtotal_sin_iva',
TRUNCATE ((`cabecera_venta`.`subtotal_con_iva` + cabecera_venta.`subtotal_sin_iva`),2)  AS 'Subtotal' , 
TRUNCATE (cabecera_venta.`iva_total`,2) AS 'Iva',
TRUNCATE (cabecera_venta.`descuento_total`,2) AS 'Descuento',
TRUNCATE (cabecera_venta.`total`,2)AS 'Total'
FROM `cabecera_venta` INNER JOIN `usuario` 
ON
cabecera_venta.`id_usuario` = usuario.`id` INNER JOIN `empresa`
ON
cabecera_venta.`id_empresa` = empresa.`id`
]]>
	</queryString>
